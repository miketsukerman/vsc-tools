Servicename: {{ item.name }}
-> {{ item.description }}
{% if item.namespaces is iterable %}
{% for n in item.namespaces %}
   Namespace: {{ n.name }}
   -> {{ n.description }}   
   {% if n.structs is iterable %}
   {% for x in n.structs %}
      Struct: {{ x.name }}
      -> {{ x.description }}
      {% for x in x.members %}
         member: {{ x.name }} (of type {{x.datatype}}) 
      {% endfor %}
   {% endfor %}
   {% endif %}
   {% if n.typedefs is iterable %}
   {% for x in n.typedefs %}
      Typedef: {{ x.name }}
      -> {{ x.description }} 
      -> Datatype: {{ x.datatype }}
      -> Min: {{ x.min }}
      -> Max: {{ x.max }}
   {% endfor %}
   {% endif %}
   {% if n.enumerations is iterable %}
   {% for x in n.enumerations %}
      Enum: {{ x.name }}
      -> {{ x.description }} 
      {% for x in x.options %}
         Options name: {{ x.name }} (of value {{x.value}}) 
      {% endfor %}
   {% endfor %}
   {% endif %}
   {% if n.methods is iterable %}
   {% for x in n.methods %}
      Method: {{ x.name }}
      -> {{ x.description }}
      {% if x.input is iterable %}
      {% for x in x.input %}
         in: {{ x.name }} (of type {{x.datatype}}) 
      {% endfor %}
      {% endif %}
      {{ gen(x.output) }}
   {% endfor %}
   {% endif %}
   {% if n.events is iterable %}
   {% for x in n.events %}
      Event: {{ x.name }}
      -> {{ x.description }}
      {% if x.input is iterable %}
      {% for x in x.input %}
         in: {{ x.name }} (of type {{x.datatype}}) 
      {% endfor %}
      {% endif %}
   {% endfor %}
   {% endif %}
   {% if n.properties is iterable %}
   {% for x in n.properties %}
      Property: {{ x.name }}
      -> {{ x.description }}
      -> Type: {{ x.datatype }}
   {% endfor %}
   {% endif %}
{% endfor %}
{% endif %}
